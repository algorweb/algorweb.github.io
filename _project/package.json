{
  "name": "template-project",
  "version": "0.1.0",
  "description": "шаблон для быстрого старта проекта на html/css/js",
  "devDependencies": {
    "@htmlacademy/editorconfig-cli": "^1.0.0",
    "autoprefixer": "^9.0.1",
    "browser-sync": "^2.24.6",
    "del": "^3.0.0",
    "eslint": "^5.2.0",
    "eslint-config-htmlacademy": "^0.5.2",
    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.1",
    "gulp-csso": "^3.0.0",
    "gulp-htmlmin": "^4.0.0",
    "gulp-imagemin": "^4.1.0",
    "gulp-plumber": "^1.1.0",
    "gulp-postcss": "7.0.1",
    "gulp-posthtml": "^3.0.0",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^4.0.1",
    "gulp-svgstore": "^6.1.0",
    "gulp-uglify": "^3.0.0",
    "gulp-webp": "^3.0.0",
    "postcss-reporter": "5.0.0",
    "postcss-scss": "1.0.5",
    "posthtml-include": "^1.1.0",
    "remove-svg-properties": "^0.3.3",
    "run-sequence": "^2.2.0",
    "stylelint": "9.2.1",
    "stylelint-declaration-block-no-ignored-properties": "1.1.0",
    "stylelint-order": "0.8.1",
    "stylelint-scss": "3.1.0"
  },
  "scripts": {
    "editorconfig": "editorconfig-cli",
    "stylelint": "stylelint 'src/sass/**/*.scss' --syntax scss",
    "eslint": "eslint src/js/**/*.js",
    "test": "npm run editorconfig && gulp lintScss && npm run eslint",
    "build": "gulp build",
    "start": "npm run build && gulp serve"
  },
  "browserslist": [
    "last 2 versions",
    "IE 11",
    "Firefox ESR"
  ],
  "editorconfig-cli": [
    "*.html",
    "*.json",
    "*.js",
    "src/js/**/*.js",
    "src/img/**/*.svg",
    "src/sass/**/*.{sass,scss}"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ge11ert/template-project.git"
  },
